<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>
	<class name="sale.sale.EndSale" table="endsale">
		<id name="id" column="id" type="long">
			<generator class="increment" />
		</id>
		<property name="videntifier" column="vIdentifier" type="string" />
		<property name="bidentifier" column="bIdentifier" type="string" />
		<property name="salesman" column="salesman" type="string" />
		<property name="saleDate" column="saleDate" type="date" />
		<property name="saleBooth" column="saleBooth" type="string" />
		<property name="effective" column="effective" type="short" />
	</class>
</hibernate-mapping>