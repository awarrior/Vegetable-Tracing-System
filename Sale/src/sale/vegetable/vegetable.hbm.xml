<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>
	<class name="sale.vegetable.Vegetable" table="vegetable">
		<id name="id" column="id" type="long">
			<generator class="increment" />
		</id>
		<property name="identifier" column="vIdentifier" type="string" />
		<property name="name" column="vName" type="string" />
		<property name="note" column="note" type="string" />
		<property name="effective" column="effective" type="short" />
	</class>
</hibernate-mapping>